	<subDataset name="Deposit&amp;Refund DataSet" uuid="c1abaece-1f90-4204-9c4c-6764c006a037">
		<parameter name="deposit_param1" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[select
    bm.id as billNO,
    bdr.id as refNo,
    bdr.transaction_type as type,
    concat(bdr.description,'-',DATE_FORMAT( bdr.transaction_date,'%d-%M-%y'))as details,
    bdr.transaction_date as  transactionDate,
   (case when bdr.description in('Deposit','Payment Towards Refund Entry') then bdr.debit_amount
	     when bdr.description in ('Refund','Refund Adjustment towards Service Balance') then -(bdr.credit_amount)
        end) AS amount,
    m.account_no as accountNo,
    m.display_name as displayName,
    m.email as email
from
    b_bill_master bm
        join
    b_deposit_refund bdr ON bm.id=bdr.bill_id and bdr.client_id = bm.client_id
         join
     m_client m ON m.id=bdr.client_id and bm.client_id=m.id where bm.id=$P{deposit_param1}]]>
		</queryString>
		<field name="billNO" class="java.lang.Integer"/>
		<field name="refNo" class="java.lang.Integer"/>
		<field name="type" class="java.lang.String"/>
		<field name="details" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="transactionDate" class="java.sql.Timestamp"/>
		<field name="amount" class="java.math.BigDecimal">
