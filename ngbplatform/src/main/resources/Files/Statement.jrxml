	<subDataset name="Invoice DataSet" uuid="0565a855-e95a-4e17-9fbf-6f30b8ef4fc6">
		<parameter name="Invoicetable_param1" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT  bm.id as billId,
        bc.id AS chargeId,
        bc.order_id as orderId,
       (CASE
        WHEN bc.priceline_id = 0 AND bc.charge_type = 'NRC' THEN btm.item_description
        WHEN bc.priceline_id = -1 AND bc.charge_type = 'NRC' THEN concat('ST','-',ph.property_code)
        ELSE pm.plan_description
      END) AS package,
      concat(DATE_FORMAT(bc.charge_start_date,'%d/%m/%y'),' - ',DATE_FORMAT(bc.charge_end_date,'%d/%m/%y')) AS period,
       (CASE
        WHEN bc.priceline_id = 0 AND bc.order_id=bos.id AND bc.charge_type = 'NRC' THEN bos.quantity
        ELSE concat(DATEDIFF(bc.charge_end_date,bc.charge_start_date)+1,' days') END ) as days,
       bc.netcharge_amount as chargeAmount,
       bcx.tax_amount as taxAmount,
      (bc.netcharge_amount + ifnull(bcx.tax_amount,0)) as totalChageAmount,
       m.account_no as accountNo,
       m.display_name as displayName,
       m.email as email
FROM  b_bill_master bm  JOIN
       b_charge bc ON bm.id = bc.bill_id AND bm.client_id = bc.client_id
       JOIN b_invoice bi ON bm.id=bi.bill_id and bi.id=bc.invoice_id
       JOIN m_client m ON m.id=bi.client_id AND bm.client_id=m.id AND m.id=bc.client_id
       LEFT JOIN b_charge_tax bcx ON bm.id=bcx.bill_id AND bcx.charge_id=bc.id
       LEFT JOIN b_onetime_sale bos  ON bc.order_id = bos.id
       LEFT JOIN b_item_master btm  ON btm.id = bos.item_id
       LEFT JOIN b_orders bo ON  bc.order_id=bo.id AND bm.client_id = bo.client_id
       LEFT JOIN b_plan_master pm ON bo.plan_id = pm.id
       LEFT JOIN b_property_history ph  ON bc.order_id=ph.id where bm.id=$P{Invoicetable_param1}]]>
		</queryString>
		<field name="billId" class="java.lang.Integer"/>
		<field name="chargeId" class="java.lang.Integer"/>
		<field name="orderId" class="java.lang.Integer"/>
		<field name="package" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="period" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="days" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="chargeAmount" class="java.lang.Double"/>
		<field name="taxAmount" class="java.lang.Double"/>
		<field name="totalChageAmount" class="java.lang.Double"/>
		<field name="accountNo" class="java.lang.String"/>
		<field name="displayName" class="java.lang.String"/>
		<field name="email" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<variable name="totalInvoice" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{totalChageAmount}]]></variableExpression>
			<initialValueExpression><![CDATA[0.00]]></initialValueExpression>
		</variable>
	</subDataset>
