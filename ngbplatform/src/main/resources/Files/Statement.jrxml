	<subDataset name="Payment DataSet" uuid="fcdda585-11b9-413b-bdda-30dde8bf499e">
		<parameter name="pay_param1" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
    bm.id AS billNO,
    bp.id AS refNo,
    mcv.code_value as payMode,
   (CASE
   WHEN bp.ref_id IS NOT NULL THEN
    concat(TRIM('Payment'from mcv.code_value),'-Payment Cancelled','-',DATE_FORMAT(bp.payment_date,'%d-%M-%y') )
    ELSE concat('Payment received through','-',TRIM('Payment' from    mcv.code_value),'-',DATE_FORMAT(bp.payment_date,'%d-%M-%y') )
     END)AS details,
   (CASE
   WHEN bp.ref_id IS NOT NULL THEN 'DEBIT'
  ELSE 'CREDIT ' END) As transaction,
   round(bp.amount_paid,2) AS amount,
    bp.remarks AS remarks,
    m.account_no as accountNo,
    m.display_name as displayName,
    m.email as email
FROM
    b_bill_master bm
        JOIN
    b_payments bp ON bm.id=bp.bill_id AND bp.client_id = bm.client_id
       join
     m_client m ON m.id=bp.client_id and bm.client_id=m.id
        LEFT JOIN
    m_code_value mcv ON bp.paymode_id = mcv.id where bm.id=$P{pay_param1}]]>
		</queryString>
		<field name="billNO" class="java.lang.Integer"/>
		<field name="refNo" class="java.lang.Integer"/>
		<field name="payMode" class="java.lang.String"/>
		<field name="details" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="transaction" class="java.lang.String"/>
		<field name="amount" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="remarks" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="accountNo" class="java.lang.String"/>
		<field name="displayName" class="java.lang.String"/>
		<field name="email" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<variable name="totalPay" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{amount}]]></variableExpression>
			<initialValueExpression><![CDATA[0.00]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="Adjustment Dataset" uuid="eeea2421-4f58-4698-9b9f-fd2b7a5234dd">
		<parameter name="adjustment_param1" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    bm.id as billNO,
    ba.id as refNo,
    concat(mcv.code_value,'-',DATE_FORMAT(ba.adjustment_date,'%d-%M-%y'))as details,
    ba.adjustment_type as type,
    ba.adjustment_date as adjustementDate,
    if((ba.adjustment_type='DEBIT'),(0-ba.adjustment_amount),ba.adjustment_amount) as amount,
    ba.remarks as remarks,
    m.account_no as accountNo,
    m.display_name as displayName,
    m.email as email
from
    b_bill_master bm
        join
    b_adjustments ba ON bm.id=ba.bill_id and ba.client_id = bm.client_id
         join
     m_client m ON m.id=ba.client_id and bm.client_id=m.id
        left join
    m_code_value mcv ON ba.adjustment_code = mcv.id where bm.id=$P{adjustment_param1}]]>
		</queryString>
		<field name="billNO" class="java.lang.Integer"/>
		<field name="refNo" class="java.lang.Integer"/>
		<field name="details" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
