	<subDataset name="New Dataset 1" uuid="7f5f4db1-2aee-480f-b2cb-30a5582e1fb4">
		<parameter name="table_param1" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT  bc.id AS Id,
bc.order_id as orderId,
round((bc.netcharge_amount+ifnull(bcx.tax_amount,0)),2) AS Amount,
       (CASE
        WHEN bc.priceline_id = 0 AND bc.charge_type = 'NRC' THEN btm.item_description
        WHEN bc.priceline_id = -1 AND bc.charge_type = 'NRC' THEN concat('ST','-',ph.property_code)
        ELSE pm.plan_description
      END) AS Description,
     (CASE
      WHEN bc.priceline_id >=1 THEN bo.active_date
      WHEN bc.priceline_id=0 AND bc.charge_type='NRC' AND bos.device_mode='NEWSALE' THEN bos.sale_date
      ELSE ph.transaction_date
      END) As Activation,
    if((bo.user_action='DISCONNECTION'), bo.end_date,null) AS Disconnection,
concat(date_format(bc.charge_start_date,'%d/%m/%y'),'-',date_format(bc.charge_end_date,'%d/%m/%y')) AS Period
    FROM b_invoice  bi JOIN
         b_charge bc ON bi.id = bc.invoice_id and bi.client_id=bc.client_id
        LEFT JOIN b_charge_tax bcx ON bc.id=bcx.charge_id and bcx.invoice_id=bi.id
        LEFT JOIN b_onetime_sale bos  ON bc.order_id = bos.id
       LEFT JOIN b_item_master btm  ON btm.id = bos.item_id
       LEFT JOIN b_orders bo ON  bc.order_id=bo.id AND bc.client_id = bo.client_id
       LEFT JOIN b_plan_master pm ON bo.plan_id = pm.id
       LEFT JOIN b_property_history ph  ON bc.order_id=ph.id
 WHERE bi.id =$P{table_param1}
GROUP BY bc.id]]>
		</queryString>
		<field name="Id" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="orderId" class="java.lang.Integer"/>
		<field name="Amount" class="java.lang.Double">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Description" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="Activation" class="java.sql.Timestamp"/>
		<field name="Disconnection" class="java.sql.Timestamp"/>
		<field name="Period" class="java.lang.String"/>
		<group name="Id">
			<groupExpression><![CDATA[$F{Id}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="param1" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="realPath" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    i.id AS invoiceId,
    i.invoice_date AS invoiceDate,
    i.invoice_amount as totalInvoiceAmount,
    mc.id as clientId,
    mc.account_no as accountNo,
    mc.display_name as displayName,
    mc.external_id as nationalId,
    cd.address_no as addressNo,
    cd.city AS city,
    cd.state AS state,
    cd.country AS country,
    cd.zip AS zip
FROM
    b_invoice i
    JOIN m_client mc ON (mc.id=i.client_id)
    LEFT JOIN  b_client_address cd  ON (mc.id=cd.client_id and cd.address_key ='PRIMARY')
    WHERE i.id=$P{param1}]]>
	</queryString>
